    <ControlTemplate x:Key="CloseButtonTemplate">
        <Image Stretch="Fill" Source="/SmartTaskChain;component/config/images/Close.png"/>
        <ControlTemplate.Triggers>
            <Trigger Property="ButtonBase.IsPressed" Value="True">
